<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.itbeien.merchant.mapper.common.IPayScenesMapper">

	<select id="getList" resultType="cn.itbeien.common.entity.pay.PayScenes">
		SELECT
		<trim suffixOverrides=",">
			ps.SCENE_CODE sceneCode,
			ps.SCENE_NAME sceneName,
		</trim>
		FROM dt_pay_scenes ps
	</select>

</mapper>